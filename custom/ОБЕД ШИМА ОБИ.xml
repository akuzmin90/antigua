<?xml version="1.0" encoding="UTF-8"?>
<p:root xmlns:p="http://www.hitmakers.com/aof/customPlaces"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.hitmakers.com/aof/customPlaces ../../xsd/customPlaces.xsd">

    <!-- ОБЕД ШИМА ОБИ  -->

    <p:place id="pi_1o_Antigua_lc_shima_obi_lunch" atlasId="pi_1o_PLACES_pi_1o_lumberjack_camp_2" imageId="pi_1o_Antigua_lc_shima_obi" name="Шима Оби_big"
             minPlayerStair="40" meetProbability="0">
        <p:blockingCreature probability="0"/>
        <p:states>

            <!-- стандартная реплика  -->
            <p:state id="default" closable="false">
                <p:talks loop="true" onClick="IGNORE">
                    <p:talk text="*Кушает*" delayMs="9000"/>
                </p:talks>
                <p:dialog>
                    <p:replica id="r1">
                        <p:mainText>Ты всегда вламываешься в чужие дома, когда хозяева хотят отдохнуть или пообедать? Не
                            трогай меня, или я познакомлю тебя со своим топором. Как раз заточила.
                        </p:mainText>
                        <p:answers>
                            <p:answer text="На обед его заточи" goto="r1" out="true"/>
                        </p:answers>
                    </p:replica>
                </p:dialog>
            </p:state>

        </p:states>
    </p:place>

</p:root>
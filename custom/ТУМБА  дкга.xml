        xsi:schemaLocation="http://www.hitmakers.com/aof/customPlaces ../../xsd/customPlaces.xsd">

    <!-- Тумба  -->

    <p:place id="pi_1o_home_storeceeper_tumba" animType="EMPTY" imageId="52x136" name="Тумба" minPlayerStair="40" meetProbability="0">
        <p:blockingCreature probability="0"/>
        <p:states>

            <!-- стандартная реплика  -->
            <p:state id="default">
                <p:dialog>
                    <p:replica id="r1" atlasId="" imageId="card-eagle-small" imageComment="Орел">
                        <p:mainText>Вы осматриваете тумбу - в ворохе бумаг и исподнего вы обнаруживаете карту орла! </p:mainText>

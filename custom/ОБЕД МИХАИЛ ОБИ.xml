<?xml version="1.0" encoding="UTF-8"?>
<p:root xmlns:p="http://www.hitmakers.com/aof/customPlaces"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.hitmakers.com/aof/customPlaces ../../xsd/customPlaces.xsd">

    <!-- ОБЕД МИХАИЛ ОБИ  -->

    <p:place id="pi_1o_Antigua_lc_michael_obi_lunch" atlasId="pi_1o_PLACES_pi_1o_lumberjack_camp_2" imageId="pi_1o_Antigua_lc_michael_obi_big" name="Михаил Оби"
             minPlayerStair="40" meetProbability="0">
        <p:blockingCreature probability="0"/>
        <p:states>

            <!-- стандартная реплика  -->
            <p:state id="default" closable="false">
                <p:talks loop="true" onClick="IGNORE">
                    <p:talk text="Я обедаю, не мешай!" delayMs="2000"/>
                </p:talks>
                <p:dialog>
                    <p:replica id="r1">
                        <p:mainText>Даже во время обеда я не могу оторвать глаз от моей милой Касуми! Как же прекрасно,
                            что случай свёл нас - и жена прекрасная, и красота невиданная, и воин она - лучше некуда!
                        </p:mainText>
                        <p:answers>
                            <p:answer text="Главное, доволен" goto="r1" out="true"/>
                        </p:answers>
                    </p:replica>
                </p:dialog>
            </p:state>

        </p:states>
    </p:place>

</p:root>
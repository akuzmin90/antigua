<?xml version="1.0" encoding="UTF-8"?>
<p:root xmlns:p="http://www.hitmakers.com/aof/customPlaces"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.hitmakers.com/aof/customPlaces ../../xsd/customPlaces.xsd">

    <!-- ТАВЕРНА ПРОСТО БОЧКА -->

    <p:place id="pi_1o_tat_just_barrel" animType="EMPTY" imageId="48x56" name="Просто бочка" minPlayerStair="40"
             meetProbability="0">
        <p:blockingCreature probability="0"/>
        <p:states>

            <!-- стандартная реплика  -->
            <p:state id="default" closable="false">
                <p:dialog>
                    <p:replica id="r1">
                        <p:mainText>Это просто бочка. Пустая. Совершенно. Можно залезть в неё, но смысл? Вы залезаете и
                            недолго в ней сидите. Кажется, все должны смеяться... Но вы видите взгляды понимания. Что ж,
                            не вы перв[ый|ая]...
                        </p:mainText>
                        <p:answers>
                            <p:answer text="Комфортно же" goto="r1" out="true"/>
                        </p:answers>
                    </p:replica>
                </p:dialog>
            </p:state>

        </p:states>
    </p:place>

</p:root>
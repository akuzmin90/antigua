        xsi:schemaLocation="http://www.hitmakers.com/aof/customPlaces ../../xsd/customPlaces.xsd">

    <!-- Работница низ  -->

    <p:place id="pi_1o_Antigua_sp_worker_women_1" animType="EMPTY" imageId="64x91" name="Работница" minPlayerStair="40"
             meetProbability="0">
        <p:blockingCreature probability="0"/>
        <p:states>

            <!-- стандартная реплика  -->
            <p:state id="default">
                <p:talks onClick="IGNORE">
                    <p:talk text="Работать с песней приятнее!" delayMs="6000"/>
